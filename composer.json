{
    "name": "lurza/laravel-id-obfuscator",
    "description": "Laravel command that helps you find routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Brecht Vermeersch",
            "email": "brechtvermeersch@outlook.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lurza\\IdObfuscator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lurza\\IdObfuscator\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "8.1",
        "spatie/laravel-package-tools": "^1.9",
        "hashids/hashids": "^4.1",
        "lurza/data-set-with-callback": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "orchestra/testbench": "^6.23",
        "nunomaduro/larastan": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
              "Lurza\\IdObfuscator\\IdObfuscatorServiceProvider"
            ],
            "aliases": {
                "Obfuscator": "IdObfuscator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
